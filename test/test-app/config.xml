<?xml version='1.0' encoding='utf-8'?>
<widget id="jp.co.getit.preview.inhouse" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Preview</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="http://localhost:3000" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <edit-config file="*-Info.plist" mode="merge" target="NSLocationWhenInUseUsageDescription">
            <string>アプリの利用には位置情報の取得許可が必要です。</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSCameraUsageDescription">
            <string>アプリがカメラへのアクセスを必要としています</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
            <string>アプリがフォトライブラリへのアクセスを必要としています</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryAddUsageDescription">
            <string>アプリがフォトライブラリへ写真を保存するためにアクセスを必要としています</string>
        </edit-config>
        <preference name="deployment-target" value="9.0" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="AppendUserAgent" value="cordova" />
    <preference name="AllowInlineMediaPlayback" value="true"/>
    <plugin name="cordova-plugin-multidex" spec="https://github.com/podtrackers/cordova-multidex" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-wkwebview-engine" spec="https://github.com/apache/cordova-plugin-wkwebview-engine" />
    <plugin name="cordova-plugin-wkwebview-inputfocusfix" spec="^1.0.2" />
    <plugin name="cordova-plugin-device" spec="^1.1.7" />
    <plugin name="cordova-plugin-appinfo-sync" spec="~0.2.0" />
    <plugin name="cordova-plugin-splashscreen" spec="~5.0.2" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="default" />
    <plugin name="cordova-open-native-settings" spec="^1.5.0" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.2" />
    <plugin name="cordova-plugin-browsertab" spec="^0.1.1" />
    <plugin name="phonegap-plugin-push" spec="^1.11.1" />
    <plugin name="cordova-plugin-keyboard" spec="~1.2.0" />
    <plugin name="cordova-plugin-camera" spec="^4.0.3" />
    <plugin name="cordova-plugin-social-message" spec="~0.4.0" />
    <plugin name="cordova-plugin-media" spec="^5.0.2" />
    <plugin name="cordova-plugin-localization" spec="0.0.1" />
    <engine name="android" spec="^7.2.0-nightly.2018.7.10.b6e4598e" />
    <engine name="ios" spec="^4.6.0-nightly.2018.7.10.46da4bc3" />
</widget>